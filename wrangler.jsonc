{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "cf-messenger",
  "main": "worker/src/index.ts",
  "compatibility_date": "2024-09-23",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "directory": "./dist",
    "binding": "ASSETS",
  },
  "observability": {
    "enabled": true,
  },
  "kv_namespaces": [
    {
      "binding": "CF_MESSENGER_SESSIONS",
      "id": "d2cede18d4b444b7986f535f9497ff6b",
      "preview_id": "xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx",
    },
  ],
  "durable_objects": {
    "bindings": [
      {
        "name": "CHAT_ROOM",
        "class_name": "ChatRoom",
      },
      {
        "name": "PRESENCE_ROOM",
        "class_name": "PresenceRoom",
      },
    ],
  },
  "migrations": [
    {
      "tag": "v1",
      "new_classes": ["ChatRoom", "PresenceRoom"],
    },
  ],
  "ai": {
    "binding": "AI",
  },
  "vars": {
    "AI_API_BASE": "http://127.0.0.1:1234/v1",
    "AI_MODEL": "@cf/meta/llama-3.2-1b-instruct",
  },
  "env": {
    "production": {
      "name": "cf-messenger-prod",
      "kv_namespaces": [
        {
          "binding": "CF_MESSENGER_SESSIONS",
          "id": "d2cede18d4b444b7986f535f9497ff6b",
        },
      ],
      "durable_objects": {
        "bindings": [
          {
            "name": "CHAT_ROOM",
            "class_name": "ChatRoom",
          },
          {
            "name": "PRESENCE_ROOM",
            "class_name": "PresenceRoom",
          },
        ],
      },
      "ai": {
        "binding": "AI",
      },
      "vars": {
        "AI_API_BASE": "",
        "AI_MODEL": "@cf/meta/llama-3.2-1b-instruct",
      },
    },
  },
}
